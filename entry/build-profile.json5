{
  "apiType": "stageMode",
  "buildOption": {
    "arkOptions": {
      "runtimeOnly": {
        "packages": [
          "@tradplus/csj_adapter",
          "@tradplus/gdt_adapter",
          "@tradplus/sigmob_adapter",
          "@tradplus/huawei_adapter",
          "@tradplus/ks_adapter"
        ]
      },
    },
    //cpp相关编译配置
    "externalNativeOptions": {
      "abiFilters": [
        "arm64-v8a",
        "x86_64",
      ]
      //用于设置本机的ABI编译环境
    }
  },
  "buildOptionSet": [
    {
      "name": "release",
      "arkOptions": {
        "obfuscation": {
          "ruleOptions": {
            "enable": false,
            "files": [
              "./obfuscation-rules.txt"
            ]
          }
        }
      }
    },
  ],
  "targets": [
    {
      "name": "default"
    },
    {
      "name": "ohosTest",
    }
  ]
}